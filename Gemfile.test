source 'https://rubygems.org'

gem "ajlisp", "0.0.0"
gem "bmabey-clispec", "0.0.2"
gem "lisp"
gem "lispy"
gem "minilisp"
gem "relisp"
gem 'jbuilder', '~> 1.2'
gem "rubylisp"
gem "rubymotionlisp"
gem "shlisp_tools"
gem "yarlisp"

group :doc do
	gem 'sdoc', require: false
end

group :development, :test do
	gem 'better_errors'
	gem 'debugger'
	gem 'pry'
end

gem "mocha", group: :test
